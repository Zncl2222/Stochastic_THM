    "\n",
    "            return data \n",
    "        else:\n",
    "            data=[]\n",
    "            for i in range(len(x.iloc[0,:])-1):\n",
    "                locals()[\"C\"+str(i)]=pd.read_table(rdata+'MC_Data'+str(i)+'.txt',header=None,encoding='gb2312',sep='\\s+')\n",
